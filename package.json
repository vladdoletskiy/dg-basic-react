{
  "name": "dg-basic-react",
  "home": "https://yevhen-komarov.github.io/dg-basic-react/",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-gh-pages": "tsc && vite build --base=/dg-basic-react/",
    "preview": "vite preview",
    "prettier": "prettier --write .",
    "lint": "yarn lint:style && yarn lint:deps && yarn lint:ts",
    "lint:style": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:deps": "depcruise --config .dependency-cruiser.js src --output-type err-long",
    "lint:ts": "tsc --noEmit"
  },
  "dependencies": {
    "effector": "^22.3.0",
    "effector-react": "^22.0.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-react": "^1.3.0",
    "dependency-cruiser": "^11.7.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-effector": "^0.6.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^28.1.0",
    "prettier": "2.6.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}
